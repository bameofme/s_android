library_path=lib_imx_c2_videodec.so;
$

@
component_name=c2.imx.h263.decoder;
library_path=lib_imx_c2_videodec.so;
$

@
component_name=c2.imx.mpeg2.decoder;
library_path=lib_imx_c2_videodec.so;
$

@
